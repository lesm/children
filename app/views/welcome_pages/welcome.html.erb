<div>
  <% if current_user_social %>
    <h1>Informacion personal</h1>
    <ul>
      <li><strong>Name:</strong> <%= current_user_social.name %></li>
      <li><strong>Provider:</strong> <%= current_user_social.provider %></li>
      <li><strong>UID:</strong> <%= current_user_social.uid %></li>
      <li><strong>Location:</strong> <%= current_user_social.location %></li>
      <li><strong>Avatar URL:</strong> <%= current_user_social.image_url %></li>
      <li><strong>URL:</strong> <%= current_user_social.url %></li>
    </ul>
      <div class="col-md-10 pull-right", style="padding-top: 30px; padding-bottom: 50px;">
      <%= image_tag 'child.jpg', class: "img-responsive" %>
    </div>
  <% elsif current_user %>
    <br/><br/>
    <div class="col-md-10 pull-right", style="padding-top: 30px; padding-bottom: 50px;">
      <%= image_tag 'child.jpg', class: "img-responsive" %>
    </div>
  <% else %>
    <h1>Necesitas Registrarte o Iniciar Sesi√≥n</h1>
  <% end %>
</div>
